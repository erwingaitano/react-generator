{
  "name": "react-generator",
  "version": "0.0.3",
  "description": "Command line folder-file generators for react projects",
  "main": "./index.js",
  "bin": {
    "react-generator": "./index.js"
  },
  "scripts": {
    "test": "./node_modules/.bin/_mocha ./**/*.spec.js",
    "coverage": "./node_modules/.bin/istanbul cover ./node_modules/.bin/_mocha ./**/*.spec.js",
    "test-travis-ci-coveralls": "./node_modules/.bin/istanbul cover ./node_modules/mocha/bin/_mocha ./**/*.spec.js --report lcovonly -- -R spec && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage"
  },
  "keywords": [
    "react",
    "generator"
  ],
  "author": "Erwin Gaitan O <erwingaitano@gmail.com>",
  "license": "ISC",
  "dependencies": {
    "commander": "^2.9.0",
    "fs-extra": "^0.26.7",
    "mkdirp": "^0.5.1",
    "node-dir": "^0.1.11"
  },
  "devDependencies": {
    "coveralls": "^2.11.8",
    "istanbul": "^0.4.2",
    "mocha": "^2.4.5",
    "mocha-lcov-reporter": "^1.2.0",
    "should": "^8.2.2"
  }
}
